{
  "compilerOptions": {
    "module": "es2020",
    "target": "ES2016",
    "jsx": "react",
    "baseUrl": "./",
    "resolveJsonModule": true,
    "allowSyntheticDefaultImports":true,
    "paths": {
      "@/*": ["src/*"]       
    },
    "noImplicitAny": false, // 允许隐式的any类型
    "moduleResolution":"node"
  },
}

